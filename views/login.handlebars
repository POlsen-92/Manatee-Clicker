<div class="card">
  <h3 class="card-header">Login</h3>
  <form id="login-form" class="card-body">
      <input id="username-login" type="text" placeholder="Username" autofocus="true" class="form-input" />
      <br>
      <input id="password-login" type="password" placeholder="Password" autofocus="true" class="form-input" />
      <br>
      <button type="submit" class="btn">Login</button>
  </form>
  <br>
  <div class="card">
      <h3 class="card-header">Create An Account</h3>
      <form id="login-form" class="card-body">
          <input id="username-create" type="text" placeholder="Username" autofocus="true" class="form-input" />
          <br>
          <input id="password-create" type="password" placeholder="Password" autofocus="true" class="form-input" />
          <br>
          <button type="submit" class="btn">Start Manateeing!</button>
          <script src="/login.js"></script>
      </form>
  </div>
  <script type="text/javascript" src="/js/login.js"></script>